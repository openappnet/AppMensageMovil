      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="EnviarMensajeResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="EnviarMensajeResult" type="xs:boolean" />

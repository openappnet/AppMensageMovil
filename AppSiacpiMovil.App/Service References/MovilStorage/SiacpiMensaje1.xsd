      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="EnviarEmailResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="EnviarEmailResult" type="xs:boolean" />
      </xs:sequence>
    </xs:complexType>
